{
  "name": "git-discord",
  "version": "1.0.3",
  "description": "Integração discord",
  "main": "src/main.js",
  "scripts": {
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format:check": "npm-run-all lint:check prettier:check",
    "format": "npm-run-all lint:fix prettier:fix",
    "dev": "nodemon",
    "prepare": "husky install",
    "check-files-staged": "node src/scripts/check-staged.js",
    "commit": "yarn check-files-staged && lint-staged && gitmoji -c",
    "lint-staged": "lint-staged"
  },
  "_moduleAliases": {
    "@common": "src",
    "@database": "database",
    "@package": "package"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --check"
    ],
    "*.jsx": [
      "eslint --fix",
      "prettier --check"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --check"
    ],
    "*.tsx": [
      "eslint --fix",
      "prettier --check"
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "boxen": "5.1.2",
    "chalk": "4",
    "discord.js": "^14.11.0",
    "dotenv": "^16.5.0",
    "glob": "^10.3.1",
    "graphql-tag": "^2.12.6",
    "i18next": "^23.2.6",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "mongoose": "^7.3.1",
    "ora": "5",
    "pg": "^8.15.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "gitmoji-cli": "^8.4.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "nodemon": "^1.14.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3"
  }
}
